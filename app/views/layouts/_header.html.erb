<!-- Navigation & Logo-->
<div class="mainmenu-wrapper">
  <div class="container">
    <div class="menuextras">
      <div class="extras">
        <ul>
          <% if current_user.present? %>
            <% if current_user.profile.class == Business %>
              <li><%= link_to 'Profile', business_path(current_user.profile) %></li>  
            <% else %>
              <li><%= link_to 'Profile', customer_path(current_user.profile) %></li>  
            <% end %>  
            <li><%= link_to 'Log Out', destroy_user_session_path, :"data-method"=>"delete" %></li>
          <% else %>
            <li><%= link_to "Log In", new_user_session_path %><br /></li>  
            <li><%= link_to 'Customer Sign Up', customer_signup_path %></li>  
            <li><%= link_to 'Business Sign Up', business_signup_path %></li>  
          <% end %>
        </ul>
      </div>
    </div>
    <nav id="mainmenu" class="mainmenu">
      <ul>
        <li class="logo-wrapper"><a href="#"><%= image_tag 'logo.png' %></a></li>
      </ul>
    </nav>
  </div>
</div>
